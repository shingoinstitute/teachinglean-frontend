{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "lean-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "_styles": [
      ],
      "styles": [
        "../node_modules/summernote/dist/summernote.css",
        "../node_modules/angular-material/angular-material.min.css",
        "lean-app-theme.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/materialize-css/bin/materialize.js",
        "../node_modules/moment/moment.js",
        "../node_modules/trix/dist/trix.js",
        "../node_modules/lodash/lodash.min.js",
        "../node_modules/summernote/dist/summernote.min.js",
        "../node_modules/angular/angular.js",
        "../node_modules/angular-route/angular-route.js",
        "../node_modules/angular-aria/angular-aria.js",
        "../node_modules/angular-animate/angular-animate.js",
        "../node_modules/angular-material/angular-material.js",
        "../node_modules/angular-messages/angular-messages.js",
        "../node_modules/angular-sanitize/angular-sanitize.js",
        "../node_modules/angular-cookies/angular-cookies.min.js",
        "../node_modules/angular-trix/dist/angular-trix.js",
        "../node_modules/angular-moment/angular-moment.js",
        "../node_modules/angular-summernote/dist/angular-summernote.min.js",
        "app/angularjs/app.js",
        "app/angularjs/controllers/MainController.js",
        "app/angularjs/controllers/AddCommentController.js",
        "app/angularjs/controllers/AddEntryController.js",
        "app/angularjs/controllers/AdminController.js",
        "app/angularjs/controllers/AnswerController.js",
        "app/angularjs/controllers/AuthController.js",
        "app/angularjs/controllers/CommentController.js",
        "app/angularjs/controllers/DashboardController.js",
        "app/angularjs/controllers/EmailController.js",
        "app/angularjs/controllers/EntryDetailController.js",
        "app/angularjs/controllers/FlagContentController.js",
        "app/angularjs/controllers/ModeratorController.js",
        "app/angularjs/controllers/NavController.js",
        "app/angularjs/controllers/ProfileController.js",
        "app/angularjs/controllers/QuestionController.js",
        "app/angularjs/controllers/ResetController.js",
        "app/angularjs/controllers/SettingsController.js",
        "app/angularjs/directives/AdminDirective.js",
        "app/angularjs/directives/AnswerDirective.js",
        "app/angularjs/directives/CommentDirective.js",
        "app/angularjs/directives/EntrySummaryDirective.js",
        "app/angularjs/directives/FlagDirective.js",
        "app/angularjs/directives/IsSameDirective.js",
        "app/angularjs/directives/ModeratorDirective.js",
        "app/angularjs/directives/ProfileDirective.js",
        "app/angularjs/directives/QuestionDirective.js",
        "app/angularjs/directives/SidenavDirective.js",
        "app/angularjs/directives/ToolbarDirective.js",
        "app/angularjs/services/authService.js",
        "app/angularjs/services/entryService.js",
        "app/angularjs/services/flagService.js",
        "app/angularjs/services/userService.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "sass",
    "component": {
    }
  }
}
